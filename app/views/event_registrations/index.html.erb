              <div class="tz_image_title_meetup">
                <img src="/events/images/line-text1.jpg" alt="line">
              </div>
            </div>
            <div>
              <%= simple_form_for(@user ||= User.new, :as => :user, :url => event_registrations_path(@tuser),
                                  :html => {:class => 'form-horizontal' },:validate=>true,:wrapper => :horizontal_form) do |f| %>
                  <%= render 'shared/error_messages', object: f.object %>
                  <%= f.error_notification %>
                  <%= display_base_errors @user%>
                  <%= f.input :name, :required => true ,:placeholder=> "Full name", :as => :string%>
                  <%= f.input :email, :required => true ,:placeholder=> "Email address",:type=> 'email', :as => :string%>
                  <%= f.input :password, :required => true ,:placeholder=> "Password"%>
                  <%= f.input  :phone_no,:placeholder=> "Phone number", :required => true, :as => :string%>
                  <%= f.simple_fields_for :participants do |participant| %>
                      <%= participant.input :college_name, :as => :string ,:placeholder=> "College name",:label => "College"%>
                      <%= participant.input :event_name ,collection: Participant::EVENTS, :prompt => "- Please select an event -", :required => true%>
                  <% end  %>
                  <%= f.input  :is_donor,:placeholder=> "Join me as a blood donor",
                               :label => "I would like to become a blood donor", :input_html => { :checked => true }%>
                  <div id="donors-details-box" class="donor-details">
                    <%= f.input :blood_group,collection: User::BLOOD_GROUPS , :prompt => "- Choose blood group -"%>
                    <%= f.input :state_id,collection: State.order(:name),:prompt => "- Select state -"%>
                    <%= f.input :district_id,collection: State.order(:name), as: :grouped_select, group_method: :districts ,:prompt => "- Select district -"%>
                  </div>
                  <%= f.button :submit, "Register to the context" , :class => 'btn btn-primary'%>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
